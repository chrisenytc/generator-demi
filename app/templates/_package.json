{
  "name": "<%= _.slugify(appName) %>",
  "description": "<%= appDescription %>",
  "version": "<%= appVersion %>",
  "homepage": "https://github.com/<%= userName %>/<%= _.slugify(appName) %>",
  "private": true,
  "author": {
    "name": "<%= authorName %>",
    "email": "<%= authorEmail %>"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/<%= userName %>/<%= _.slugify(appName) %>.git"
  },
  "bugs": {
    "url": "https://github.com/<%= userName %>/<%= _.slugify(appName) %>/issues"
  },
  "licenses": [
    {
      "type": "BSD",
      "url": "https://github.com/<%= userName %>/<%= _.slugify(appName) %>/blob/master/LICENSE"
    }
  ],
  "main": "app.js",
  "engines": {
    "node": ">= 0.10.0"
  },
  "scripts": {
    "start": "NODE_ENV=production ./node_modules/forever/bin/forever -m 5 app.js",
    "startcluster": "NODE_ENV=production ./node_modules/forever/bin/forever -m 5 cluster.js",
    "test": "NODE_ENV=test gulp test"
  },
  "dependencies": {
    "express": "~3.4.7",
    "express-resource": "~1.0.0",
    "demi-logger": "~0.1.0",
    "bella": "~0.3.0",
    "underscore": "~1.5.2",
    "underscore.string": "~2.3.3",
    "colors": "~0.6.2",
    "cors": "~2.1.1",
    "mongoose": "~3.8.3",
    "forever": "~0.10.11"
  },
  "devDependencies": {
    "mocha": "~1.10.0",
    "chai": "~1.8.0",
    "supertest": "~0.8.1",
    "gulp": "~3.5.0",
    "gulp-jshint": "~1.3.4",
    "gulp-mocha": "~0.4.1",
    "jshint-stylish": "~0.1.5"
  },
  "keywords": ["web", "api", "restful", "framework"]
}
